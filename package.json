  "devDependencies": {
    "flow-bin": "^0.64.0",
    "gh-pages": "^1.1.0",
    "nwb": "^0.21.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
